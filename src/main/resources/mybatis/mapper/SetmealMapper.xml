<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wuzhicheng.pigstore.mapper.SetmealMapper">

    <select id="selectByCid" resultType="com.wuzhicheng.pigstore.entity.Setmeal">
